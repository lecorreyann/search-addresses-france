{
  "name": "@lecorreyann/search-addresses-france",
  "description": "Search french addresses",
  "version": "1.0.2",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "type": "module",
  "exports": {
