  },
  "homepage": "https://github.com/lecorreyann/search-addresses-france#readme",
  "devDependencies": {
    "@alcalzone/esm2cjs": "^1.1.2",
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "node-fetch": "^3.3.2"
